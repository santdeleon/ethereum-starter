{
  "name": "@santdeleon/ethereum-starter",
  "version": "1.1.0",
  "description": "Boilerplate to easily get started building Ethereum applications with Truffle",
  "main": "truffle-config.js",
  "author": "Santiago Deleon",
  "license": "MIT",
  "keywords": [
    "santdeleon",
    "ethereum",
    "ethereum-starter",
    "truffle"
  ],
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@openzeppelin/contract-loader": "^0.6.2",
    "@openzeppelin/contracts": "^3.3.0",
    "@openzeppelin/contracts-ethereum-package": "^3.0.0",
    "@openzeppelin/solidity-loader": "^1.5.1",
    "@openzeppelin/test-helpers": "^0.5.10",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "husky": "^4.3.6",
    "openzeppelin-solidity": "^3.3.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "truffle-assertions": "^0.9.2"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern '**/*.js'",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "directories": {
    "test": "test"
  }
}
